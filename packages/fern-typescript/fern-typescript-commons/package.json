{
  "name": "@fern-typescript/commons",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:fern-api/fern.git",
    "directory": "packages/fern-typescript/fern-typescript-commons"
  },
  "files": [
    "lib"
  ],
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -r ./lib",
