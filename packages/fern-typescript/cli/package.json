{
  "name": "fern-typescript",
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "sideEffects": false,
  "bin": "./cli",
  "scripts": {
    "clean": "tsc --build --clean tsconfig.esm.json && tsc --build --clean tsconfig.json",
    "compile": "yarn run compile:cjs",
    "compile:all": "run-p compile:esm compile:cjs",
    "compile:esm": "tsc --build tsconfig.esm.json && echo '{ \"type\": \"module\" }' > lib/esm/package.json",
    "compile:cjs": "tsc --build tsconfig.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json",
    "test": "yarn run compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck",
    "dockerTagLocal": "./docker/create_docker.sh local",
    "dockerTagVersion": "./docker/create_docker.sh $(yarn info fern-typescript --all --json | jq -r .children.Version)"
  },
  "dependencies": {
    "@fern-api/commons": "workspace:*",
    "@fern-api/generator-runner": "workspace:*",
    "@fern-typescript/client": "workspace:*",
    "@fern-typescript/commons": "workspace:*",
    "@fern-typescript/helper-manager": "workspace:*",
    "@fern-typescript/model": "workspace:*",
    "memfs": "^3.4.1",
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@fern-api/api": "workspace:*",
    "@ts-morph/common": "^0.13.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^17.0.33",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.12.0",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "simple-progress-webpack-plugin": "^2.0.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "umd-compat-loader": "^2.1.2",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
