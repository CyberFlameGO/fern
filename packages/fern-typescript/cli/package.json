{
  "name": "fern-typescript",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:fern-api/fern.git",
    "directory": "packages/fern-typescript/cli"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck",
    "dockerTagLocal": "./docker/create_docker.sh local",
    "dockerTagVersion": "./docker/create_docker.sh"
  },
  "dependencies": {
    "@fern-api/config-management-commons": "workspace:*",
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@fern-fern/generator-logging-api-client": "^0.0.25",
    "@fern-typescript/client": "workspace:*",
    "@fern-typescript/commons": "workspace:*",
    "@fern-typescript/helper-manager": "workspace:*",
    "@fern-typescript/model": "workspace:*",
    "@fern-typescript/server": "workspace:*",
    "execa": "^5.1.1",
    "memfs": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@fern-fern/ir-model": "0.0.3",
    "@fern-typescript/testing-utils": "workspace:*",
    "@ts-morph/common": "^0.13.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "node-loader": "^2.0.0",
    "prettier": "^2.7.1",
    "simple-progress-webpack-plugin": "^2.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4",
    "umd-compat-loader": "^2.1.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0"
  }
}
