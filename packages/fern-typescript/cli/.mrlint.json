{
  "type": "cli",
  "private": false,
  "rules": {
    "package-json": {
      "scripts": {
        "dockerTagLocal": "./docker/create_docker.sh local",
        "dockerTagVersion": "./docker/create_docker.sh $(yarn info fern-typescript --all --json | jq -r .children.Version)"
      }
    },
    "depcheck": {
      "ignores": ["webpack-cli", "umd-compat-loader", "@ts-morph/common", "ts-loader", "ts-node"],
      "ignore-patterns": ["docker/bundle.js"]
    }
  }
}
