{
    "name": "@fern-api/api",
    "version": "0.1.2",
    "files": [
        "lib"
    ],
    "main": "lib/cjs/index.js",
    "types": "lib/esm/index.d.ts",
    "exports": {
        ".": {
            "require": "lib/cjs/index.js",
            "default": "lib/esm/index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsc --project tsconfig.json && tsc --project tsconfig.cjs.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json && echo '{ \"type\": \"module\" }' > lib/esm/package.json"
    },
    "devDependencies": {
        "@types/node": "^17.0.33",
        "typescript": "^4.6.4"
    }
}
