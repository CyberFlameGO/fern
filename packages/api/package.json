{
  "name": "@fernapi/api-generation",
  "private": true,
  "scripts": {
    "clean": "tsc --build --clean && /bin/rm -r generated",
    "codegen": "mkdir -p generated && fern generate src generated/ir.json && run codegen:typescript",
    "codegen:typescript": "fern-typescript model generated/ir.json generated/typescript"
  },
  "devDependencies": {
    "@fernapi/fern": "0.1.3",
    "@fernapi/fern-typescript": "^0.1.9"
  }
}
